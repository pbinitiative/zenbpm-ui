{
  "name": "zenbpm-ui",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prebuild": "pnpm generate:api && node scripts/generate-i18n-types.mjs",
    "predev": "pnpm prebuild",
    "dev": "vite",
    "dev:mocks": "pnpm dev --mode mocks",
    "dev:live": "pnpm dev --mode live",
    "build": "tsc -b && vite build",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "check": "pnpm lint && pnpm typecheck",
    "preview": "vite preview",
    "generate:validation-schemas": "node scripts/generate-openapi-validation-schemas.mjs",
    "generate:api": "pnpm generate:validation-schemas && orval",
    "generate:i18n-types": "node scripts/generate-i18n-types.mjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@bpmn-io/form-js": "^1.18.0",
    "@bpmn-io/properties-panel": "^3.34.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@lezer/highlight": "^1.2.3",
    "@lezer/xml": "^1.0.6",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@tanstack/react-query": "^5.90.12",
    "axios": "^1.13.2",
    "bpmn-js": "^18.9.1",
    "bpmn-js-properties-panel": "^5.44.0",
    "camunda-bpmn-js": "^5.15.0",
    "camunda-bpmn-js-behaviors": "^1.12.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "dmn-js": "^17.5.0",
    "dmn-js-properties-panel": "^3.9.0",
    "dotenv": "^17.3.1",
    "i18next": "^25.7.1",
    "json-bigint": "^1.0.0",
    "oidc-client-ts": "^3.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.4.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@redocly/cli": "^2.19.2",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@types/json-bigint": "^1.0.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.11",
    "globals": "^16.5.0",
    "msw": "^2.12.4",
    "openapi-typescript": "^7.13.0",
    "openapi3-ts": "^4.5.0",
    "orval": "^7.13.2",
    "storybook": "^10.1.11",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
